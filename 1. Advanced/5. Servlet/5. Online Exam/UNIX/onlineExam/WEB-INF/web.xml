<web-app>
	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>ProgLoginPage</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>check</servlet-name>
		<servlet-class>ProgLoginCheck</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>check</servlet-name>
		<url-pattern>/check</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>exam</servlet-name>
		<servlet-class>ProgExamPage</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>exam</servlet-name>
		<url-pattern>/exam</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>check_exam</servlet-name>
		<servlet-class>ProgExamCheck</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>check_exam</servlet-name>
		<url-pattern>/check_exam</url-pattern>
	</servlet-mapping>
</web-app>

